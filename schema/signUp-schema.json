            "examples": [
                "kevin820125@gmail.com"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "12345"
            ]
        },
        "photourl": {
            "$id": "#/properties/photourl",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "data:image/jpeg;base64"
            ],
            "title": "The photourl schema",
